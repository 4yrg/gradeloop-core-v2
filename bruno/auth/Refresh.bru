meta {
  name: Refresh
  type: http
  seq: 2
}

# Rotate refresh token and obtain new access + refresh tokens
post {
  url: "http://localhost:8081/api/auth/refresh"
  headers {
    "Content-Type": "application/json"
  }
  body {
{
  "refresh_token": "<refresh-token>"
}
  }
  auth: inherit
}

settings {
  encodeUrl: true
}
